# 代理IP扫描器环境变量示例文件
# 复制此文件为.env并根据环境修改相应配置

# API密钥（主密钥）
API_KEY=your_secret_api_key

# 数据库配置
DB_HOST=db
DB_PORT=3306
DB_USER=proxy_scanner
DB_PASSWORD=your_db_password
DB_NAME=proxy_scanner

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_redis_password

# JWT配置
JWT_SECRET=your_jwt_secret
JWT_EXPIRE_SECONDS=3600

# HMAC密钥
HMAC_SECRET=your_hmac_secret

# 管理员账户
ADMIN_USERNAME=admin
ADMIN_PASSWORD=secure_admin_password

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/proxy_scanner.log

# 扫描配置
SCAN_THREADS=200
SCAN_TIMEOUT=5
SCAN_INTERVAL=300

# 端口配置
API_PORT=8000 